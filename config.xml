<?xml version='1.0' encoding='utf-8'?>
<widget id="com.timesvideo.gaanesuneansune" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>HelloWorld</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="android-minSdkVersion" value="15" />
    <preference name="android-maxSdkVersion" value="28" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <preference name="onesignal" value="" />
<preference name="admobBannerId" value="ca-app-pub-2607313420010336/7643001475" />
<preference name="admobInterstitialId" value="ca-app-pub-2607313420010336/2217508304" />
    <engine name="android" spec="^7.1.2" />
    <plugin name="cordova-plugin-admobpro" spec="2.35.3">
        <variable name="PLAY_SERVICES_VERSION" value="16.0.0" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
       <plugin name="onesignal-cordova-plugin" spec="2.3.3" /> 
   <plugin name="cordova-plugin-statusbar" spec="https://github.com/apache/cordova-plugin-statusbar.git" /> 
  <plugin name="cordova-plugin-network-information" spec="~2.0.1" /> 
  <plugin name="cordova-plugin-customconfigparameters" spec="~3.0.0" /> 
  <plugin name="cordova-plugin-app-version" spec="~0.1.9" /> 
   <plugin name="cordova-plugin-screen-orientation" spec="~3.0.1" /> 
  <plugin name="com.bunkerpalace.cordova.YoutubeVideoPlayer" spec="https://github.com/JonSmart/CordovaYoutubeVideoPlayer.git " /> 
<plugin name="com.santinowu.cordova.imagedownloader" spec="https://github.com/Santino-Wu/cordova-plugin-imagedownloader.git"> 
<variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="explain why you require accessing Photo" /> 
</plugin>
 <plugin name="cordova-plugin-shortcuts-android" spec="~0.1.0" />


    <plugin name="cordova-android-play-services-gradle-release" spec="^1.4.6" />
</widget>
